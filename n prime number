def prime(n):
    print("Prime numbers until " + str(n) + " are : ")
    for i in range(2,n+1):
        flag = 1
        for j in range(2,(i//2)+1):
            if(i%j == 0):
                flag = 0
        if(flag == 1):
            print(i,end = " ")
n = int(input("Enter number of times to be executed : \n"))
prime(n)
